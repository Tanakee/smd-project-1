{
  "installCommand": "npm install --prefix ./geologapp",
  "buildCommand": "npm run build --prefix ./geologapp",
  "outputDirectory": "./geologapp/dist",
  "rewrites": [
    {"source": "/api/(.*)", "destination": "/geologapp/api/$1"},
    {"source": "/(.*)", "destination": "/geologapp/dist/index.html"}
  ]
}